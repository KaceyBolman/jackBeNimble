{
  "manifest_version": 2,

  "name": "Bitfinex Hotkeys",
  "description": "This extension gives hotkey order entry for the bitfinex platform.",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "config.html",
    "default_title": "Bitfinex Hotkeys"
  },

  "content_scripts": [
    {

      "matches": [
        "https://www.bitfinex.com/trading",
        "https://beta.bitfinex.com/trading"
      ],

      "css": ["main.css"],

      "js": ["jquery.js", "orderEntry.js"]

    }
  ]
}
